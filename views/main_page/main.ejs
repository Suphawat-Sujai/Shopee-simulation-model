    <title>Shopee</title>

    <style>
      .topnav {
        display: flex;
        justify-content: space-between;
        align-items: center;
        padding: 10px 20px;
        background-color: #f0f0f0;
        box-shadow: 0px 2px 5px rgba(0, 0, 0, 0.1);
      }

      .logo a {
        font-size: 26px;
        font-weight: bold;
        text-decoration: none;
        color: #c73d3d;
      }

      .search {
        flex-grow: 1;
        margin: 0 20px;
      }

      .search input {
        padding: 10px;
        width: 90%;
      }

      .search button {
        padding: 8px 16px;
        background-color: #ff4500;
        border: none;
        color: white;
        cursor: pointer;
      }

      .user a {
        margin-right: 10px;
        text-decoration: none;
        color: #333;
      }

      @media screen and (max-width: 768px) {
        .topnav {
          flex-direction: column;
          align-items: stretch;
        }

        .menu,
        .user {
          margin-top: 10px;
        }

        .search {
          margin: 10px 0;
        }
      }

      .v-center {
        align-items: center;
        display: flex;
      }

      .category-shopee {
        display: flex;
        justify-content: space-around;
        background-color: #fff;
        width: 1200px;
        margin: 10px auto 0;
        min-height: 108px;
      }

      .category-margin {
        margin: 18px auto 8px;
      }

      .category-size {
        width: 45px;
        height: 45px;
        border-radius: 50%;
      }

      .category-image {
        height: 100%;
        border-radius: 0;
      }

      .category-position {
        position: relative;
      }

      .category-text {
        color: #222;
        letter-spacing: 0;
        text-align: center;
      }

      .category-settext {
        display: -webkit-box;
        text-overflow: ellipsis;
        -webkit-box-orient: vertical;
        -webkit-line-clamp: 2;
        margin-bottom: 8px;
        word-wrap: break-word;
        overflow: hidden;
        white-space: pre-line;
        line-height: 1.1875rem;
      }
    </style>
  </head>
